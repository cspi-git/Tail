                        <TextArea fx:id="LogBoxTextBox" layoutX="26.0" layoutY="106.0" prefHeight="303.0" prefWidth="547.0" />
                        <Button layoutX="26.0" layoutY="72.0" mnemonicParsing="false" onAction="#ClearOutputButton" prefHeight="26.0" prefWidth="103.0" text="Clear output" />
                         <Button layoutX="139.0" layoutY="72.0" mnemonicParsing="false" onAction="#SaveOutputButton" prefHeight="26.0" prefWidth="103.0" text="Save output" />
                        <Button layoutX="371.0" layoutY="23.0" mnemonicParsing="false" onAction="#LookupButton" prefHeight="26.0" prefWidth="81.0" text="Lookup" />
                     </children></AnchorPane>
            </content>
          </Tab>
          <Tab text="Settings">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
